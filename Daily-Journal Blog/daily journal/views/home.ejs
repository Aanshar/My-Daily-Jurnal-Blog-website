

<!DOCTYPE html>
<html class="no-js">
    <head>
        <meta charset="utf-8">
       
        <title>DailyJournal</title>
       
        <!-- <link rel="stylesheet" href="css/styles.css"> -->
    </head>
    <body>

        <%- include("partials/header"); %>
       
        <h1>Home</h1>

        <p><%= homeStartContent %></p>

      
        <% posts.forEach(function(element){ %>
        <h1><%= element.title %></h1>
        
        <p><%= element.content.substring(0,100) + " ..."; %><a href = "/posts/ + <%= element.title %>" >Read More</a></p>
        <% }) %>

        <%- include("partials/footer"); %>
        
        
    </body>
</html>